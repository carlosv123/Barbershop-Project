@model IEnumerable<CapStone.Models.Barbershop>

@{
    ViewBag.Title = "Index";
}

<h2>Barbershop Info</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Haircutprices)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Openhours)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OpenDays)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.holyday)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.holdayDays)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.phoneNumber)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Haircutprices)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Openhours)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OpenDays)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.holyday)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.holdayDays)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.phoneNumber)
        </td>
    </tr>
}

</table>
