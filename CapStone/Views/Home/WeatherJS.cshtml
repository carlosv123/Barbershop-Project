
@{
    ViewBag.Title = "WeatherJS";
}

<h2>WeatherJS</h2>
<p id="name"></p>
<p id="temp"></p>
<p id="humidity"></p>
<button>Get Weather</button>

<script>
    $(document).ready(function () {
        $("button").click(function () {
            $.get("http://api.openweathermap.org/data/2.5/weather?q=Milwaukee&APPID=57a30f253654d8374160b752d1e17997&units=imperial", function (response) {
                 //response
                console.log(response);
                $("#name").text(response.name);
                $("#temp").text(response.main.temp);
                $("#humidity").text(response.main.humidity);
            });
        });
    });

</script>